.cart-wrapper
  width 100%
  height 100%
  overflow hidden
  padding 45px 0 90px 0
  .cart-bottom
    position fixed
    left 0
    right 0
    bottom 50px
    background-color #eee
    .all-box
      float left
      height 45px
      line-height 45px
      margin-right 30px
    .total-price
      float left
      height 45px
      line-height 45px
