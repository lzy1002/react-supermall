.home-wrapper
  width 100%
  height 100%
  padding 45px 0 50px 0
  overflow hidden
  .recommend-box
    display flex
    padding 10px 0 50px 0
    border-bottom 10px solid #eee
    .recommend-item
      flex 1
      display flex
      flex-direction column
      align-items center
      padding 0 5px
      img
        width 100%
      .text
        margin-top 10px
  .pop-box
    img
      width 100%
